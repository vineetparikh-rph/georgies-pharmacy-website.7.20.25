{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    },
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "/api/index.js"
    },
    {
      "source": "/refill",
      "destination": "/"
    },
    {
      "source": "/transfer",
      "destination": "/"
    },
    {
      "source": "/vaccine",
      "destination": "/"
    },
    {
      "source": "/:path*",
      "destination": "/"
    }
  ]
}
